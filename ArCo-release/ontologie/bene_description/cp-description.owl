    <!-- http://www.w3id.org/arco/cpdescription/hasCategory -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/arco/cpdescription/hasCategory">
        <rdfs:domain rdf:resource="http://www.w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="http://www.w3id.org/arco/cpdescription/CulturalPropertyCategory"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa un bene culturale alla sua categoria.</rdfs:comment>
        <rdfs:label xml:lang="it">ha categoria</rdfs:label>
        <rdfs:label xml:lang="en">has category</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/arco/cpdescription/hasConservationStatus -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/arco/cpdescription/hasConservationStatus">
        <rdfs:domain rdf:resource="http://www.w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="http://www.w3id.org/arco/cpdescription/ConservationStatus"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa il bene culturale al suo stato di conservazione.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates the cultural property with its conservation status.</rdfs:comment>
        <rdfs:label xml:lang="it">ha stato di conservazione</rdfs:label>
        <rdfs:label xml:lang="en">has conservation status</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/arco/cpdescription/hasConservationStatusType -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/arco/cpdescription/hasConservationStatusType">
        <rdfs:domain rdf:resource="http://www.w3id.org/arco/cpdescription/ConservationStatus"/>
        <rdfs:range rdf:resource="http://www.w3id.org/arco/cpdescription/ConservationStatusType"/>
        <rdfs:label xml:lang="it">ha tipo di stato di conservazione</rdfs:label>
        <rdfs:label xml:lang="en">has conservation status type</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/arco/cpdescription/hasCulturalPropertyDefinition -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/arco/cpdescription/hasCulturalPropertyDefinition">
        <rdfs:domain rdf:resource="http://www.w3id.org/arco/cpdescription/CulturalPropertyType"/>
        <rdfs:range rdf:resource="http://www.w3id.org/arco/cpdescription/CulturalPropertyDefinition"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega la tipologia di un bene culturale a una delle sue componenti, cioè la definizione.</rdfs:comment>
        <rdfs:label xml:lang="it">ha definizione del bene culturale</rdfs:label>
        <rdfs:label xml:lang="en">has cultural property definition</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/arco/cpdescription/hasCulturalPropertySpecification -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/arco/cpdescription/hasCulturalPropertySpecification">
        <rdfs:domain rdf:resource="http://www.w3id.org/arco/cpdescription/CulturalPropertyType"/>
        <rdfs:range rdf:resource="http://www.w3id.org/arco/cpdescription/CulturalPropertySpecification"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega la tipologia di un bene culturale a una delle sue componenti, cioè alle specifiche morfologiche e funzionali.</rdfs:comment>
        <rdfs:label xml:lang="it">ha specifiche del bene culturale</rdfs:label>
        <rdfs:label xml:lang="en">has cultural property specification</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/arco/cpdescription/hasCulturalPropertyType -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/arco/cpdescription/hasCulturalPropertyType">
        <rdfs:domain rdf:resource="http://www.w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="http://www.w3id.org/arco/cpdescription/CulturalPropertyType"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un bene culturale al tipo di bene, per permettere la corretta e precisa individuazione del bene.</rdfs:comment>
        <rdfs:label xml:lang="it">ha tipo di bene culturale</rdfs:label>
        <rdfs:label xml:lang="en">has cultural property type</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/arco/cpdescription/hasDesignationInTime -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/arco/cpdescription/hasDesignationInTime">
        <rdfs:subPropertyOf rdf:resource="http://www.w3id.org/arco/cpdescription/hasNameInTime"/>
        <rdfs:domain rdf:resource="http://www.w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="http://dati.beniculturali.it/cis/NameInTime"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa un bene culturale alla sua denominazione.</rdfs:comment>
        <rdfs:label xml:lang="it">ha designazione nel tempo</rdfs:label>
        <rdfs:label xml:lang="en">has designation in time</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/arco/cpdescription/hasEdition -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/arco/cpdescription/hasEdition">
        <rdfs:domain rdf:resource="http://www.w3id.org/arco/core/HistoricOrArtisticProperty"/>
        <rdfs:range rdf:resource="http://www.w3id.org/arco/cpdescription/Edition"/>
        <rdfs:label xml:lang="it">Questa proprietà collega un bene storico e artistico a una sua edizione.</rdfs:label>
        <rdfs:label xml:lang="it">ha edizione</rdfs:label>
        <rdfs:label xml:lang="en">has edition</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/arco/cpdescription/hasLocationSubject -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/arco/cpdescription/hasLocationSubject">
        <rdfs:domain rdf:resource="http://www.w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="http://www.w3id.org/arco/location/TimeIndexedQualifiedLocation"/>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta l&apos;area rappresentata come soggetto di un bene culturale.</rdfs:comment>
        <rdfs:label xml:lang="it">ha localizzazione rappresentata</rdfs:label>
        <rdfs:label xml:lang="en">has location as subject</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/arco/cpdescription/hasMibactScopeOfProtection -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/arco/cpdescription/hasMibactScopeOfProtection">
        <rdfs:domain rdf:resource="http://www.w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="http://www.w3id.org/arco/cpdescription/MibactScopeOfProtection"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa un bene culturale all&apos;ambito di tutela nel quale ricade.</rdfs:comment>
        <rdfs:label xml:lang="it">ha ambito di tutela</rdfs:label>
        <rdfs:label xml:lang="en">has MiBACT scope of protection</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.w3id.org/arco/cpdescription/hasNameInTime -->

    <owl:ObjectProperty rdf:about="http://www.w3id.org/arco/cpdescription/hasNameInTime">
        <rdfs:comment xml:lang="it">Questa proprietà è stata creata come proprietà generica per sottoproprietà più specifiche, per collegare un&apos;entità a una denominazione nel tempo.</rdfs:comment>
        <rdfs:label xml:lang="it">ha nome nel tempo</rdfs:label>
        <rdfs:label xml:lang="en">has name in time</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


