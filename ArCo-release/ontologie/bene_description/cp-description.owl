    <!-- http://www.w3id.org/arco/cpdescription/ConservationStatus -->

    <owl:Class rdf:about="http://www.w3id.org/arco/cpdescription/ConservationStatus">
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/l0/Characteristic"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3id.org/arco/cpdescription/hasConservationStatusType"/>
                <owl:allValuesFrom rdf:resource="http://www.w3id.org/arco/cpdescription/ConservationStatusType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3id.org/arco/cpdescription/conservationStatusSpecifications"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3id.org/arco/cpdescription/hasProposedIntervention"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3id.org/arco/cpdescription/hasStorageConditions"/>
                <owl:allValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">It represents the conservation status of the cultural property.</rdfs:comment>
        <rdfs:comment xml:lang="it">Questa classe rappresenta lo stato di conservazione di un bene culturale.</rdfs:comment>
        <rdfs:label xml:lang="en">Conservation Status</rdfs:label>
        <rdfs:label xml:lang="it">Stato di Conservazione</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:Class>
    


    <!-- http://www.w3id.org/arco/cpdescription/ConservationStatusType -->

    <owl:Class rdf:about="http://www.w3id.org/arco/cpdescription/ConservationStatusType">
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/l0/Characteristic"/>
        <rdfs:label xml:lang="en">Conservation Status Type</rdfs:label>
        <rdfs:label xml:lang="it">Tipo di stato di conservazione</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:Class>
    


    <!-- http://www.w3id.org/arco/cpdescription/CulturalPropertyCategory -->

    <owl:Class rdf:about="http://www.w3id.org/arco/cpdescription/CulturalPropertyCategory">
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/l0/Topic"/>
        <rdfs:subClassOf>
            <owl:Restriction>
