<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/arco/ontology/location/"
     xml:base="https://w3id.org/arco/ontology/location/"
     xmlns:arco-core="https://w3id.org/arco/ontology/core/"
     xmlns:opla="http://ontologydesignpatterns.org/opla#"
     xmlns:adms="https://w3id.org/italia/onto/ADMS/"
     xmlns:arco-location="https://w3id.org/arco/ontology/location/"
     xmlns:l0="https://w3id.org/italia/onto/l0/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:ns="http://www.w3.org/2006/vcard/ns#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:opla1="http://ontologydesignpatterns.org/opla/"
     xmlns:cis="http://dati.beniculturali.it/cis/"
     xmlns:clv="https://w3id.org/italia/onto/CLV/"
     xmlns:arco="https://w3id.org/arco/ontology/arco/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:arco-cd="https://w3id.org/arco/ontology/context-description/"
     xmlns:dcat="http://www.w3.org/ns/dcat#"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="https://w3id.org/arco/ontology/location/">
        <owl:imports rdf:resource="http://dati.beniculturali.it/cis/"/>
        <owl:imports rdf:resource="https://w3id.org/arco/ontology/context-description/"/>
        <owl:imports rdf:resource="https://w3id.org/arco/ontology/core/"/>
        <dc:creator>Chiara Veninata (MiBACT, ICCD)</dc:creator>
        <dc:creator>Ludovica Marinucci (ISTC-CNR, STLab)</dc:creator>
        <owl:versionInfo xml:lang="it">Versione 0.4 alpha</owl:versionInfo>
        <dc:creator>Aldo Gangemi (ISTC-CNR, STLab)</dc:creator>
        <dc:creator>Valentina Carriero (ISTC-CNR, STLab)</dc:creator>
        <dc:creator>Andrea Nuzzolese (ISTC-CNR, STLab)</dc:creator>
        <dc:creator>Marialetizia Mancinelli (MiBACT, ICCD)</dc:creator>
        <rdfs:comment xml:lang="it">Questo modulo modella le informazioni relative alle localizzazioni e alla georeferenziazione di un bene culturale.</rdfs:comment>
        <dc:creator>Valentina Presutti (ISTC-CNR, STLab)</dc:creator>
        <rdfs:label>Location Ontology (ArCo network)</rdfs:label>
        <rdfs:comment xml:lang="en">This module models information related to the localization and georeferencing of a cultural property.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/arco/ontology/arco/iccdNormTag -->

    <owl:AnnotationProperty rdf:about="https://w3id.org/arco/ontology/arco/iccdNormTag"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dati.beniculturali.it/cis/isInSite -->

    <owl:ObjectProperty rdf:about="http://dati.beniculturali.it/cis/isInSite"/>
    


    <!-- https://w3id.org/arco/ontology/context-description/hasLegalSituation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/context-description/hasLegalSituation"/>
    


    <!-- https://w3id.org/arco/ontology/core/hasType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/core/hasType"/>
    


    <!-- https://w3id.org/arco/ontology/location/atLocation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/atLocation">
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/location/TimeIndexedTypedLocation"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CLV/Feature"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una localizzazione tipizzata nel tempo al luogo cui si riferisce.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <rdfs:label xml:lang="en">at location</rdfs:label>
        <rdfs:label xml:lang="it">presso luogo</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco:iccdNormTag xml:lang="it">se LC senza sottoelemento LDC; se LA senza sottoelemento PRC</arco:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/atSite -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/atSite">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/location/atLocation"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/location/TimeIndexedTypedLocation"/>
        <rdfs:range rdf:resource="http://dati.beniculturali.it/cis/Site"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una localizzazione qualificata nel tempo al contenitore fisico cui si riferisce.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <rdfs:label xml:lang="en">at site</rdfs:label>
        <rdfs:label xml:lang="it">presso contenitore fisico</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco:iccdNormTag>LC/LDC; LA/PRC</arco:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasAltitude -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasAltitude">
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/location/Coordinates"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/location/Altitude"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega le coordinate relative alla georeferenziazione di un oggetto (Geometry) alla sua altitudine.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <rdfs:label xml:lang="it">ha altitudine</rdfs:label>
        <rdfs:label xml:lang="en">has altitude</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco:iccdNormTag xml:lang="it">quota minima: GP/GPC/GPCI - GA/GAQ/GAQI - GL/GLQ/GLQI - MT/MTA/MTAM; quota massima: GP/GPC/GPCS - GA/GAQS - GL/GLQ/GLQS - MT/MTA/MTAX; quota relativa: MT/MTA/MTAR; coordinata z: 4.00 GE/GEC/GECZ - 3.00GP/GPC/GPCL</arco:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasBaseMap -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasBaseMap">
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CLV/Geometry"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/location/BaseMap"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una georeferenziazione di un oggetto alla base cartografica eventualmente utilizzata.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <rdfs:label xml:lang="it">ha base cartografica</rdfs:label>
        <rdfs:label xml:lang="en">has base map</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco:iccdNormTag xml:lang="it">ICCD Normativa Trasversale 4.00: GE/GPB; versioni precedenti: GP/GPB - GA/GAB - GL/GLB</arco:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasCadastralCity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasCadastralCity">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/location/hasCadastralEntity"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/location/CadastralIdentity"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CLV/City"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;identità catastale di un&apos;entità al comune a cui è intestato il foglio di mappa catastale, riportato nella forma attestata nel catasto.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <rdfs:label xml:lang="it">ha città catastale</rdfs:label>
        <rdfs:label xml:lang="en">has cadastral city</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco:iccdNormTag>ICCD Normativa Trasversale 4.00: CS/CTS/CTSC</arco:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasCadastralEntity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasCadastralEntity">
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/location/CadastralIdentity"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/location/CadastralEntity"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;identità catastale di un&apos;entità a una delle entità catastali, intese come oggetti spaziali, che la definiscono.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <rdfs:label xml:lang="it">ha entità catastale</rdfs:label>
        <rdfs:label xml:lang="en">has cadastral entity</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasCadastralIdentity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasCadastralIdentity">
        <rdfs:domain rdf:resource="http://dati.beniculturali.it/cis/CulturalEntity"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/location/CadastralIdentity"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un bene culturale all&apos;insieme delle informazioni catastali che lo riguardano, relativamente a una delle sue localizzazioni (storica, attuale, di rinvenimento, etc.)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <rdfs:label xml:lang="it">ha identità catastale</rdfs:label>
        <rdfs:label xml:lang="en">has cadastral identity</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco:iccdNormTag>ICCD Normativa Trasversale 4.00: CS</arco:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasCadastreType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasCadastreType">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/core/hasType"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/location/CadastralIdentity"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/location/CadastreType"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una identità catastale relativa a un&apos;entità al tipo di catasto (es.: catasto terreni, catasto fabbricati, catasto misto)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <rdfs:label xml:lang="it">ha tipo di catasto</rdfs:label>
        <rdfs:label xml:lang="en">has cadastre type</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasClericalAdministrativeArea -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasClericalAdministrativeArea">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/CLV/hasAddressComponent"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CLV/Address"/>
        <rdfs:range rdf:resource="http://dati.beniculturali.it/cis/ClericalAdministrativeArea"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un indirizzo a un&apos;area amministrativa ecclesiastica (es.: diocesi).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <rdfs:label xml:lang="it">ha area amministrativa ecclesiastica</rdfs:label>
        <rdfs:label xml:lang="en">has clerical administrative area</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco:iccdNormTag>ICCD Normativa Trasversale 4.00: LC/PVE; LA/PRE</arco:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasContinent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasContinent">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/location/Continent"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega la localizzazione di un&apos;entità al continente in cui si trova.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <rdfs:label xml:lang="it">ha continente</rdfs:label>
        <rdfs:label xml:lang="en">has continent</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco:iccdNormTag>LA/PRT/PRTK</arco:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasCoordinates -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasCoordinates">
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CLV/Geometry"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/location/Coordinates"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega la georeferenziazione di un oggetto (Geometry) alle coordinate (x, y, z).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <rdfs:label xml:lang="it">ha coordinate</rdfs:label>
        <rdfs:label xml:lang="en">has coordinates</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco:iccdNormTag>GP/GPD; GA/GAPD; GL/GLS, ICCD Normativa Trasversale: GE/GEC</arco:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasCulturalPropertyAddress -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasCulturalPropertyAddress">
        <rdfs:domain rdf:resource="http://dati.beniculturali.it/cis/CulturalEntity"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CLV/Address"/>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta la relazione che collega un bene culturale all&apos;indirizzo della sua localizzazione fisica attuale, al momento della catalogazione. La relazione fornisce una rappresentazione semplificata, con ridotta espressività, della localizzazione attuale del bene culturale, e provoca la perdita delle altre informazioni che riguardano la collocazione del bene (eventuale contenitore fisico, continente, collocazione specifica nel contenitore fisico, etc.)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <rdfs:label xml:lang="en">cultural property address</rdfs:label>
        <rdfs:label xml:lang="it">indirizzo del bene culturale</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco:iccdNormTag>LC/PVC</arco:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasGeometryMethod -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasGeometryMethod">
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CLV/Geometry"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/location/GeometryMethod"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una georeferenziazione al metodo utilizzato per il posizionamento.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <rdfs:label xml:lang="it">ha metodo di georeferenziazione</rdfs:label>
        <rdfs:label xml:lang="en">has geometry method</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco:iccdNormTag xml:lang="it">ICCD Normativa Trasversale 4.00: GE/GPM; versioni precedenti: GP/GPM - GA/GAM - GL/GLM</arco:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasGeometryTechnique -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasGeometryTechnique">
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CLV/Geometry"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/location/GeometryTechnique"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una georeferenziazione alla tecnica di georeferenziazione.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <rdfs:label xml:lang="it">ha tecnica di georeferenziazione</rdfs:label>
        <rdfs:label xml:lang="en">has geometry technique</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco:iccdNormTag xml:lang="it">ICCD Normativa Trasversale 4.00: GE/GPT; versioni precedenti: GP/GPT - GA/GAT - GL/GLT</arco:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasGovernamentalAdministrativeArea -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasGovernamentalAdministrativeArea">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/CLV/hasAddressComponent"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CLV/Address"/>
        <rdfs:range rdf:resource="http://dati.beniculturali.it/cis/GovernamentalAdministrativeArea"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un indirizzo a un&apos;area di appartenenza amministrativa.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <rdfs:label xml:lang="it">ha area di appartenenza amministrativa</rdfs:label>
        <rdfs:label xml:lang="en">has governamental administrative area</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco:iccdNormTag>ICCD Normativa Trasversale 4.00: LS/LCS/LCSA; LA/PRT/PRTR</arco:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasHistoricalArea -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasHistoricalArea">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/CLV/hasAddressComponent"/>
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/CLV/Address"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CLV/AdministrativeUnitComponent"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un indirizzo a un&apos;area storico-geografica.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <rdfs:label xml:lang="it">ha area storica</rdfs:label>
        <rdfs:label xml:lang="en">has historical area</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasLocationType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasLocationType">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/core/hasType"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/location/LocationType"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità, per esempio un situazione in cui un luogo riveste un certo ruolo nei confronti di un bene culturale in un intervallo di tempo, o una georeferenziazione, o un&apos;identità catastale al ruolo della localizzazione (fisica attuale, precedente, di rinvenimento, etc.) cui si riferisce.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <rdfs:label xml:lang="it">ha tipo di localizzazione cui si riferisce</rdfs:label>
        <rdfs:label xml:lang="en">has referred location type</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco:iccdNormTag xml:lang="it">ICCD Normativa Trasversale 4.00: GE/GEL; LA/TLC; LA/TCL; LC (CurrentPhysicalLocation) - Normativa F: anche LR (ShotLocation) - Normative di versioni precedenti: GP/GPL; GA/GAL; GL/GLL</arco:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/location/hasSiteDefinition -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/location/hasSiteDefinition">
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/location/SiteType"/>
