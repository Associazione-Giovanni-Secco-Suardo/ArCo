     -->

    


    <!-- http://dati.gov.it/onto/l0/name -->

    <owl:DatatypeProperty rdf:about="http://dati.gov.it/onto/l0/name"/>
    


    <!-- http://www.w3id.org/arco/location/addressDetails -->

    <owl:DatatypeProperty rdf:about="http://www.w3id.org/arco/location/addressDetails">
        <rdfs:domain rdf:resource="http://dati.gov.it/onto/clvapit#Address"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="it">Questa proprietà è utilizzata per rappresentare informazioni sui percorsi che permettono l’accesso al bene o che lo delimitano, o ulteriori specifiche per la localizzazione del bene.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property is used to represent other paths and details about the address.</rdfs:comment>
        <rdfs:label xml:lang="en">address details</rdfs:label>
        <rdfs:label xml:lang="it">specifiche dell&apos;indirizzo</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/arco/location/locationSpecifications -->

    <owl:DatatypeProperty rdf:about="http://www.w3id.org/arco/location/locationSpecifications">
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta informazioni specifiche sulla collocazione fisica del bene.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property represents specific informations about the cultural property location.</rdfs:comment>
        <rdfs:label xml:lang="en">location specifications</rdfs:label>
        <rdfs:label xml:lang="en">specifiche di localizzazione</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3id.org/arco/location/siteIdentifier -->

    <owl:DatatypeProperty rdf:about="http://www.w3id.org/arco/location/siteIdentifier">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/italia/onto/l0/identifier"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://dati.beniculturali.it/cis/Site"/>
        <rdfs:range rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
        <rdfs:comment xml:lang="it"></rdfs:comment>
        <rdfs:label xml:lang="it">identificativo del contenitore fisico</rdfs:label>
        <rdfs:label xml:lang="en">site identifier</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/italia/onto/l0/identifier -->

    <owl:DatatypeProperty rdf:about="https://w3id.org/italia/onto/l0/identifier"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dati.beniculturali.it/cis/GeographicalFeature -->

    <owl:Class rdf:about="http://dati.beniculturali.it/cis/GeographicalFeature"/>
    


    <!-- http://dati.beniculturali.it/cis/Site -->

    <owl:Class rdf:about="http://dati.beniculturali.it/cis/Site"/>
    


    <!-- http://dati.gov.it/onto/clvapit#Address -->

    <owl:Class rdf:about="http://dati.gov.it/onto/clvapit#Address"/>
    


    <!-- http://dati.gov.it/onto/clvapit#Feature -->

    <owl:Class rdf:about="http://dati.gov.it/onto/clvapit#Feature"/>
    


    <!-- http://dati.gov.it/onto/tiapit/TimeInterval -->

    <owl:Class rdf:about="http://dati.gov.it/onto/tiapit/TimeInterval"/>
    


    <!-- http://www.w3id.org/arco/core/ImmovableCulturalProperty -->

    <owl:Class rdf:about="http://www.w3id.org/arco/core/ImmovableCulturalProperty"/>
    


    <!-- http://www.w3id.org/arco/location/Continent -->

    <owl:Class rdf:about="http://www.w3id.org/arco/location/Continent">
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/CLV/Feature"/>
        <rdfs:comment xml:lang="it">Questa classe rappresenta un Continente.</rdfs:comment>
        <rdfs:label xml:lang="en">Continent</rdfs:label>
        <rdfs:label xml:lang="it">Continente</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:Class>
    


    <!-- http://www.w3id.org/arco/location/HistoricalGeographicalArea -->

    <owl:Class rdf:about="http://www.w3id.org/arco/location/HistoricalGeographicalArea">
        <rdfs:subClassOf rdf:resource="http://dati.beniculturali.it/cis/GeographicalFeature"/>
        <rdfs:comment xml:lang="it">Questa classe rappresenta un&apos;area storico-geografica.</rdfs:comment>
        <rdfs:label xml:lang="it">Area storico-geografica</rdfs:label>
        <rdfs:label xml:lang="en">Historical geographical area</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:Class>
    


    <!-- http://www.w3id.org/arco/location/LocationType -->

    <owl:Class rdf:about="http://www.w3id.org/arco/location/LocationType">
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/l0/Characteristic"/>
        <rdfs:comment xml:lang="it">Questa classe rappresenta il tipo di localizzazione di un bene culturale.</rdfs:comment>
        <rdfs:label xml:lang="en">Location Type</rdfs:label>
        <rdfs:label xml:lang="it">Tipo di localizzazione</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:Class>
    


    <!-- http://www.w3id.org/arco/location/SiteDefinition -->

    <owl:Class rdf:about="http://www.w3id.org/arco/location/SiteDefinition">
        <rdfs:comment xml:lang="it">Questa classe rappresenta la definizione della tipologia di un contenitore fisico.</rdfs:comment>
        <rdfs:label xml:lang="it">Definizione di Contenitore Fisico</rdfs:label>
        <rdfs:label xml:lang="en">Site Definition</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:Class>
    


    <!-- http://www.w3id.org/arco/location/SiteSpecification -->

    <owl:Class rdf:about="http://www.w3id.org/arco/location/SiteSpecification">
        <rdfs:comment xml:lang="it">Questa classe rappresenta specifiche sulla qualificazione del contenitore fisico, cioè l&apos;aggettivo o la locuzione che, rispetto alla Site Definition, precisa, integra o caratterizza il contenitore fisico dal punto di vista della
condizione giuridico-amministrativa o funzionale.</rdfs:comment>
        <rdfs:label xml:lang="it">Qualificazione del Contenitore Fisico</rdfs:label>
        <rdfs:label xml:lang="en">Site Specification</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:Class>
    


    <!-- http://www.w3id.org/arco/location/SiteType -->

    <owl:Class rdf:about="http://www.w3id.org/arco/location/SiteType">
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/l0/Characteristic"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3id.org/arco/location/hasSiteDefinition"/>
                <owl:someValuesFrom rdf:resource="http://www.w3id.org/arco/location/SiteDefinition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3id.org/arco/location/hasSiteSpecification"/>
                <owl:allValuesFrom rdf:resource="http://www.w3id.org/arco/location/SiteSpecification"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="it">Questa classe rappresenta il tipo di contenitore fisico, cioè la tipologia del contenitore fisico ed eventuali aggettivi o locuzioni che precisano, integrano o caratterizzano il contenitore fisico dal punto di vistaq della condizione giuridico-amministrativa o funzionale.</rdfs:comment>
        <rdfs:label xml:lang="en">Site Type</rdfs:label>
        <rdfs:label xml:lang="it">Tipo di Contenitore Fisico</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:Class>
    


    <!-- http://www.w3id.org/arco/location/TimeIndexedQualifiedLocation -->

    <owl:Class rdf:about="http://www.w3id.org/arco/location/TimeIndexedQualifiedLocation">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://dati.gov.it/onto/tiapit/atTime"/>
                <owl:allValuesFrom rdf:resource="http://dati.gov.it/onto/tiapit/TimeInterval"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3id.org/arco/location/hasLocationType"/>
                <owl:allValuesFrom rdf:resource="http://www.w3id.org/arco/location/LocationType"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="it">Questa classe rappresenta una localizzazione di un bene culturale, inserita in un arco temporale e qualificata in base al ruolo che la localizzazione riveste nei confronti del bene culturale.</rdfs:comment>
        <rdfs:label xml:lang="it">Localizzazione qualificata nel tempo</rdfs:label>
        <rdfs:label xml:lang="en">Time Indexed Qualified Location</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:Class>
    


    <!-- http://www.w3id.org/arco/location/ToponymInTime -->

    <owl:Class rdf:about="http://www.w3id.org/arco/location/ToponymInTime">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://dati.gov.it/onto/tiapit/atTime"/>
                <owl:allValuesFrom rdf:resource="http://dati.gov.it/onto/tiapit/TimeInterval"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://dati.gov.it/onto/l0/name"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2000/01/rdf-schema#Literal"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="it">Questa classe rappresenta eventuali altre denominazioni del luogo.</rdfs:comment>
        <rdfs:comment xml:lang="en">This class represents other place&apos;s designations.</rdfs:comment>
        <rdfs:label xml:lang="it">Toponimo nel tempo</rdfs:label>
        <rdfs:label xml:lang="en">Toponym in time</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:Class>
    


    <!-- http://www.w3id.org/arco/location/TypeOfContext -->

    <owl:Class rdf:about="http://www.w3id.org/arco/location/TypeOfContext">
        <rdfs:subClassOf rdf:resource="https://w3id.org/italia/onto/l0/Characteristic"/>
        <rdfs:comment xml:lang="it">Questa classe rappresenta il tipo di contesto in cui si trova un bene culturale.</rdfs:comment>
        <rdfs:comment xml:lang="en">This class represents the type of context, in which a cultural property is located.</rdfs:comment>
        <rdfs:label xml:lang="it">Tipo di contesto geografico</rdfs:label>
        <rdfs:label xml:lang="en">Type of Geographic Context</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:Class>
    


