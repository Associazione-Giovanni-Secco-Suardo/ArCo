<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/arco/ontology/arco/"
     xml:base="https://w3id.org/arco/ontology/arco/"
     xmlns:arco-location="https://w3id.org/arco/ontology/location/"
     xmlns:l0="https://w3id.org/italia/onto/l0/"
     xmlns:roapit="https://w3id.org/italia/onto/RO/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:ns="http://www.w3.org/2006/vcard/ns#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:opla2="http://www.ontologydesignpatterns.org/opla/"
     xmlns:cis="http://dati.beniculturali.it/cis/"
     xmlns:CLV="https://w3id.org/italia/onto/CLV/"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:core1="https://w3id.org/arco/ontology/core/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:arco-dd="https://w3id.org/arco/ontology/denotative-description/"
     xmlns:dcat="http://www.w3.org/ns/dcat#"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:opla="http://ontologydesignpatterns.org/opla/"
     xmlns:adms="https://w3id.org/italia/onto/ADMS/"
     xmlns:tiapit="https://w3id.org/italia/onto/TI/"
     xmlns:arco="https://w3id.org/arco/ontology/arco/"
     xmlns:core="https://w3id.org/arco/core/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:arco-cd="https://w3id.org/arco/ontology/context-description/"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="https://w3id.org/arco/ontology/arco/">
        <owl:imports rdf:resource="http://dati.beniculturali.it/cis/"/>
        <owl:imports rdf:resource="https://w3id.org/arco/ontology/denotative-description/"/>
        <owl:imports rdf:resource="https://w3id.org/arco/ontology/context-description/"/>
        <owl:imports rdf:resource="https://w3id.org/arco/ontology/location/"/>
        <owl:imports rdf:resource="https://w3id.org/arco/ontology/catalogue/"/>
        <owl:imports rdf:resource="https://w3id.org/arco/ontology/cultural-event/"/>
        <dc:creator>Chiara Veninata (MiBACT, ICCD)</dc:creator>
        <dc:creator>Ludovica Marinucci (ISTC-CNR, STLab)</dc:creator>
        <rdfs:comment xml:lang="it">Questo è il modulo centrale della rete di ontologie di ArCo - Architettura della Conoscenza. Qui si modellano informazioni, relative al bene culturale, considerate centrali. Vengono importati tutti gli altri moduli della rete.</rdfs:comment>
        <rdfs:label>ArCo Ontology (ArCo network)</rdfs:label>
        <owl:versionInfo xml:lang="it">Versione 0.4 alpha</owl:versionInfo>
        <dc:creator>Aldo Gangemi (ISTC-CNR, STLab)</dc:creator>
        <dc:creator>Valentina Carriero (ISTC-CNR, STLab)</dc:creator>
        <dc:creator>Andrea Nuzzolese (ISTC-CNR, STLab)</dc:creator>
        <opla:reusesPatternAsTemplate rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl"/>
        <dc:creator>Marialetizia Mancinelli (MiBACT, ICCD)</dc:creator>
        <dc:creator>Valentina Presutti (ISTC-CNR, STLab)</dc:creator>
        <opla:reusesPatternAsTemplate rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/classification.owl#"/>
        <rdfs:comment xml:lang="en">The ArCo module is the central node of the network ArCo - Architecture of Knowledge. It imports all the other modules and the ontologies directly reused, and models core information about cultural heritage.</rdfs:comment>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://ontologydesignpatterns.org/opla/isNativeTo -->

    <owl:AnnotationProperty rdf:about="http://ontologydesignpatterns.org/opla/isNativeTo"/>
    


    <!-- http://ontologydesignpatterns.org/opla/reusesPatternAsTemplate -->

    <owl:AnnotationProperty rdf:about="http://ontologydesignpatterns.org/opla/reusesPatternAsTemplate"/>
    


    <!-- https://w3id.org/arco/ontology/arco/iccdNormTag -->

    <owl:AnnotationProperty rdf:about="https://w3id.org/arco/ontology/arco/iccdNormTag">
        <rdfs:comment xml:lang="it">Questa proprietà permette di annotare il campo o i campi della normativa ICCD corrispondenti a una determinata classe o proprietà dell&apos;ontologia.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property annotates the field or the fields of the ICCD regulation to which a specific class or property refers.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/arco/"/>
        <rdfs:label xml:lang="en">ICCD norm tag</rdfs:label>
        <rdfs:label xml:lang="it">acronimo normativa ICCD</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dati.beniculturali.it/cis/isRelatedToRiT -->

    <owl:ObjectProperty rdf:about="http://dati.beniculturali.it/cis/isRelatedToRiT"/>
    


    <!-- https://w3id.org/arco/ontology/arco/hasAlternativeDiscipline -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco/hasAlternativeDiscipline">
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/arco/CulturalProperty"/>
        <rdfs:range rdf:resource="http://dati.beniculturali.it/cis/SubjectDiscipline"/>
        <rdfs:comment xml:lang="it">Questa proprietà permette di associare un bene culturale a discipline scientifiche, alternative a quella principale, cui fa riferimento.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a cultural property to a related scientific discipline, alternative to the main one.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/arco/"/>
        <rdfs:label xml:lang="it">ha altra disciplina</rdfs:label>
        <rdfs:label xml:lang="en">has alternative discipline</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <iccdNormTag>ICCD Normativa PST: CT/CTA</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/arco/hasCataloguingAgency -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco/hasCataloguingAgency">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/arco/hasRelatedAgency"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/arco/CulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Agent"/>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta la relazione che collega un bene culturale al relativo ente schedatore. La relazione tra il ruolo e l&apos;ente che lo riveste è espressa nel nome della proprietà, che quindi ne fornisce una rappresentazione semplificata, con ridotta espressività. Nell&apos;ontologia questa relazione è modellata anche, in maniera più espressiva, dalla classe AgentRole, che permette di collegare l&apos;ente e il suo ruolo.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a cultural property to its cagaloguing agency. The relation is declared in the name of the property, but within the ontology this relation is also modeled, in a more expressive way, through the class AgentRole.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/arco/"/>
        <rdfs:label xml:lang="it">ha ente schedatore</rdfs:label>
        <rdfs:label xml:lang="en">has cataloguing agency</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <iccdNormTag>ICCD Normativa Trasversale 4.00: CD/ESC</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/arco/hasCulturalPropertyComponent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco/hasCulturalPropertyComponent">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/arco/hasPart"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/arco/isCulturalPropertyComponentOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/arco/ComplexCulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/arco/CulturalPropertyComponent"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un bene culturale complesso a una delle sue parti componenti.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a cultural property to its components.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/arco/"/>
        <rdfs:label xml:lang="it">ha parte componente di bene culturale</rdfs:label>
        <rdfs:label xml:lang="en">has cultural property component</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <iccdNormTag>OG/OGT/OGTP</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/arco/hasCulturalPropertyResidual -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco/hasCulturalPropertyResidual">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/arco/hasPart"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/arco/isCulturalPropertyResidualOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/arco/CulturalProperty"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/arco/ontology/arco/CulturalPropertyResidual"/>
                    <rdf:Description rdf:about="https://w3id.org/arco/ontology/arco/SomeCulturalPropertyResiduals"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:comment xml:lang="it">Questa proprietà collega un bene culturale alla/alle sua/e parte/i residuale/i.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a cultural property to its residual.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/arco/"/>
        <rdfs:label xml:lang="it">ha parte residuale di bene culturale</rdfs:label>
        <rdfs:label xml:lang="en">has cultural property residual</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <iccdNormTag>OG/OGT/OGTW</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/arco/hasHeritageProtectionAgency -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco/hasHeritageProtectionAgency">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/ontology/arco/hasRelatedAgency"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/arco/CulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Agent"/>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta la relazione che collega un bene culturale al relativo ente competente per la tutela. La relazione tra il ruolo e l&apos;ente che lo riveste è espressa nel nome della proprietà, che quindi ne fornisce una rappresentazione semplificata, con ridotta espressività. Nell&apos;ontologia questa relazione è modellata anche, in maniera più espressiva, dalla classe AgentRole, che permette di collegare l&apos;ente e il suo ruolo.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a cultural property to its heritage protection agency. The relation is declared in the name of the property, but within the ontology this relation is also modeled, in a more expressive way, through the class AgentRole.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/arco/"/>
        <rdfs:label xml:lang="it">ha ente competente per la tutela</rdfs:label>
        <rdfs:label xml:lang="en">has heritage protection agency</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <iccdNormTag>ICCD Normativa Trasversale 4.00: CD/ECP</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/arco/hasKeyword -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco/hasKeyword">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/arco/Keyword"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità a una parola chiave relativa, che può essere utilizzata per la ricerca dell&apos;entità.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates an entity to its keyword, used to search for that entity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/arco/"/>
        <rdfs:label xml:lang="it">ha parola chiave</rdfs:label>
        <rdfs:label xml:lang="en">has keyword</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <iccdNormTag>ICCD Normativa PST: CT/CTC</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/arco/hasLocation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco/hasLocation">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CLV/Feature"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa un&apos;entità a un&apos;area geografica in cui l&apos;entità è collocata.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates an entity to the geographic area in which that entity is located.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/arco/"/>
        <rdfs:label xml:lang="it">ha luogo</rdfs:label>
        <rdfs:label xml:lang="en">has location</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <iccdNormTag xml:lang="it">DO/MST/MSTL (versioni precedenti a 4.00)</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/arco/hasMainDiscipline -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco/hasMainDiscipline">
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/arco/CulturalProperty"/>
        <rdfs:range rdf:resource="http://dati.beniculturali.it/cis/SubjectDiscipline"/>
        <rdfs:comment xml:lang="it">Questa proprietà permette di associare un bene culturale alla disciplina scientifica più importante, o storicamente riconosciuta, cui fa riferimento.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a cultural property to the main related scientific discipline.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/arco/"/>
        <rdfs:label xml:lang="it">ha disciplina principale</rdfs:label>
        <rdfs:label xml:lang="en">has main discipline</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <iccdNormTag>ICCD Normativa PST: CT/CTP</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/arco/hasMibacScopeOfProtection -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco/hasMibacScopeOfProtection">
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/arco/CulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/arco/MibacScopeOfProtection"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa un bene culturale all&apos;ambito di tutela nel quale ricade.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a cultural property to ist MiBAC scope of protection</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/arco/"/>
        <rdfs:label xml:lang="it">ha ambito di tutela</rdfs:label>
        <rdfs:label xml:lang="en">has MiBAC scope of protection</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <iccdNormTag>ICCD Normativa Trasversale 4.00: OG/AMB</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/arco/hasMusicalInstrumentClassification -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco/hasMusicalInstrumentClassification">
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/arco/MusicHeritage"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/arco/MusicalInstrumentClassification"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un bene musicale a una classificazione con cui lo strumento può essere codificato.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/arco/"/>
        <rdfs:label xml:lang="it">ha classificazione di strumento musicale</rdfs:label>
        <rdfs:label xml:lang="en">has musical instrument classification</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/arco/hasNumberOfComponents -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco/hasNumberOfComponents">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/arco/isNumberOfComponentsOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/arco/ComplexCulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/arco/NumberOfComponents"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un bene culturale complesso alla quantità delle parti componenti del bene stesso.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a complex cultural property to the quantity associated to the components of the complex cultural property.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/arco/"/>
        <rdfs:label xml:lang="it">ha numero di componenti</rdfs:label>
        <rdfs:label xml:lang="en">has number of components</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <iccdNormTag>OG/QNT/QNTN; OG/QNT/QNTI; OG/QNT/QNTS</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/arco/hasNumberOfCulturalPropertyPart -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco/hasNumberOfCulturalPropertyPart">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/arco/isNumberOfCulturalPropertyPartOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/arco/CulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/arco/NumberOfCulturalPropertyPart"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un bene culturale alla quantità associata a una specifica parte del bene stesso (es.: il numero di denti di un reperto antropologico, dove il numero viene associato alla parte &quot;dente&quot;, per quantificarla).</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates the cultural property to the quantity associated to a specific cultural property part.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/arco/"/>
        <rdfs:label xml:lang="it">ha numero di parte di bene culturale</rdfs:label>
        <rdfs:label xml:lang="en">has number of cultural property part</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <iccdNormTag>ICCD Normativa AT 3.01: EL/ELE/ELEM</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/arco/hasPart -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco/hasPart">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/ontology/arco/isPartOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#TransitiveProperty"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <opla:isNativeTo rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità a una sua parte. Per esempio, può collegare un bene culturale a una sua parte (es.: piano terra, manico, etc.)</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates an entity to its part, e.g. a cultural property to one of its parts.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/arco/"/>
        <rdfs:label xml:lang="it">ha parte</rdfs:label>
        <rdfs:label xml:lang="en">has part</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <iccdNormTag xml:lang="it">&quot;Riferimento alla parte&quot;, presente in molti campi strutturati delle normative ICCD</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/arco/hasPhotographicHeritageClassification -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco/hasPhotographicHeritageClassification">
        <rdfs:domain rdf:resource="https://w3id.org/arco/ontology/arco/PhotographicHeritage"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/ontology/arco/PhotographicHeritageClassification"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un bene fotografico a una classificazione di un bene fotografico secondo un sistema di classificazione alternativo a Iconclass, che sia comunque riconosciuto e utilizzato a livello nazionale, o preferibilmente internazionale.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/ontology/arco/"/>
        <rdfs:label xml:lang="it">ha classificazione di bene fotografico</rdfs:label>
        <rdfs:label xml:lang="en">has photographic heritage classification</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <iccdNormTag>ICCD Normativa F 4.00: SG/CLF</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/ontology/arco/hasPhotographicHeritageClassificationType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/ontology/arco/hasPhotographicHeritageClassificationType">
