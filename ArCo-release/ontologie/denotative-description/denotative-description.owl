<?xml version="1.0"?>
<rdf:RDF xmlns="https://w3id.org/arco/denotative-description/"
     xml:base="https://w3id.org/arco/denotative-description/"
     xmlns:arco-core="https://w3id.org/arco/core/"
     xmlns:opla="http://www.ontologydesignpatterns.org/opla/"
     xmlns:adms="https://w3id.org/italia/onto/ADMS/"
     xmlns:l0="https://w3id.org/italia/onto/l0/"
     xmlns:arco-location="http://www.w3id.org/arco/location/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:ns="http://www.w3.org/2006/vcard/ns#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:clv="https://w3id.org/italia/onto/CLV/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:dcat="http://www.w3.org/ns/dcat#"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="https://w3id.org/arco/denotative-description/">
        <owl:imports rdf:resource="http://dati.beniculturali.it/cis/"/>
        <owl:imports rdf:resource="https://w3id.org/arco/core/"/>
        <dc:creator>Chiara Veninata (MiBACT, ICCD)</dc:creator>
        <dc:creator>Aldo Gangemi (ISTC-CNR, STLab)</dc:creator>
        <dc:creator>Valentina Carriero (ISTC-CNR, STLab)</dc:creator>
        <owl:versionInfo xml:lang="it">Versione 0.3 beta</owl:versionInfo>
        <dc:creator>Andrea Nuzzolese (ISTC-CNR, STLab)</dc:creator>
        <opla:reusesPatternAsTemplate rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/descriptionandsituation.owl"/>
        <rdfs:comment xml:lang="it">Questo modulo, detto della descrizione denotativa del bene culturale (denotazione è inteso come l&apos;atto di indicare una cosa attraverso segni esteriori), include la modellazione delle informazioni che riguardano il bene culturale in sé, con i suoi attributi (rilevati/rilevabili al momento della catalogazione).</rdfs:comment>
        <dc:creator>Marialetizia Mancinelli (MiBACT, ICCD)</dc:creator>
        <dc:creator>Valentina Presutti (ISTC-CNR, STLab)</dc:creator>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.ontologydesignpatterns.org/opla/isNativeTo -->

    <owl:AnnotationProperty rdf:about="http://www.ontologydesignpatterns.org/opla/isNativeTo"/>
    


    <!-- http://www.ontologydesignpatterns.org/opla/reusesPatternAsTemplate -->

    <owl:AnnotationProperty rdf:about="http://www.ontologydesignpatterns.org/opla/reusesPatternAsTemplate"/>
    


    <!-- https://w3id.org/arco/core/iccdNormTag -->

    <owl:AnnotationProperty rdf:about="https://w3id.org/arco/core/iccdNormTag"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/arco/context-description/hasAuthor -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/context-description/hasAuthor"/>
    


    <!-- https://w3id.org/arco/core/hasType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/core/hasType"/>
    


    <!-- https://w3id.org/arco/denotative-description/hasAccessory -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasAccessory">
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/MusicHeritage"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/denotative-description/Accessory"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un bene musicale a un suo accessorio, ovvero un oggetto collegabile al bene musicale, sia nel caso in cui sia funzionale alla produzione del suono, sia nel caso in cui sia di supporto alla produzione del suono o alla conservazione o alla prassi musicale del bene.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/denotative-description/"/>
        <rdfs:label xml:lang="it">ha accessorio</rdfs:label>
        <rdfs:label xml:lang="en">has accessory</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco-core:iccdNormTag>ICCD Normativa SM 4.00: AS/ASS/ASST</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/denotative-description/hasConservationStatus -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasConservationStatus">
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/denotative-description/ConservationStatus"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa il bene culturale al suo stato di conservazione.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates the cultural property with its conservation status.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/denotative-description/"/>
        <rdfs:label xml:lang="it">ha stato di conservazione</rdfs:label>
        <rdfs:label xml:lang="en">has conservation status</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco-core:iccdNormTag>CO/STC</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/denotative-description/hasConservationStatusType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasConservationStatusType">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/core/hasType"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/denotative-description/ConservationStatus"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/denotative-description/ConservationStatusType"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa lo stato di conservazione a uno dei valori del tipo di stato di conservazione.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/denotative-description/"/>
        <rdfs:label xml:lang="it">ha tipo di stato di conservazione</rdfs:label>
        <rdfs:label xml:lang="en">has conservation status type</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco-core:iccdNormTag>CO/STC/STCC</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/denotative-description/hasCulturalPropertyDefinition -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasCulturalPropertyDefinition">
        <rdfs:domain rdf:resource="https://w3id.org/arco/denotative-description/CulturalPropertyType"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/denotative-description/CulturalPropertyDefinition"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega la tipologia di un bene culturale a una delle sue componenti, cioè la definizione.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/denotative-description/"/>
        <rdfs:label xml:lang="it">ha definizione del bene culturale</rdfs:label>
        <rdfs:label xml:lang="en">has cultural property definition</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco-core:iccdNormTag>OG/OGT/OGTD</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/denotative-description/hasCulturalPropertySpecification -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasCulturalPropertySpecification">
        <rdfs:domain rdf:resource="https://w3id.org/arco/denotative-description/CulturalPropertyType"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/denotative-description/CulturalPropertySpecification"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega la tipologia di un bene culturale a una delle sue componenti, cioè alle specifiche morfologiche e funzionali.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/denotative-description/"/>
        <rdfs:label xml:lang="it">ha specifiche del bene culturale</rdfs:label>
        <rdfs:label xml:lang="en">has cultural property specification</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco-core:iccdNormTag>OG/OGT/OGTT</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/denotative-description/hasCulturalPropertyType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasCulturalPropertyType">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/core/hasType"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/denotative-description/CulturalPropertyType"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un bene culturale al tipo di bene, per permettere la corretta e precisa individuazione del bene.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/denotative-description/"/>
        <rdfs:label xml:lang="it">ha tipo di bene culturale</rdfs:label>
        <rdfs:label xml:lang="en">has cultural property type</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco-core:iccdNormTag>OG/OGT</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/denotative-description/hasDesignationInTime -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasDesignationInTime">
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/denotative-description/DesignationInTime"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa un bene culturale alla sua denominazione e/o dedicazione o, per la normativa FF, titolo, nel corso del tempo.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/denotative-description/"/>
        <rdfs:label xml:lang="it">ha designazione nel tempo</rdfs:label>
        <rdfs:label xml:lang="en">has designation in time</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag>ICCD Normativa Trasversale 4.00: OG/OGD</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/denotative-description/hasDesignationType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasDesignationType">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/core/hasType"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/denotative-description/DesignationInTime"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/denotative-description/DesignationType"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/denotative-description/"/>
        <rdfs:label xml:lang="it">ha tipo di denominazione</rdfs:label>
        <rdfs:label xml:lang="en">has designation type</rdfs:label>
        <owl:versionInfo xml:lang="it">Questa proprietà collega una denominazione o dedicazione o, per la normativa FF, titolo nel tempo al tipo di denominazione del bene (altra lingua, attribuita, attuale, dialettale, etc.)</owl:versionInfo>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag>ICCD Normativa Trasversale 4.00: OG/OGD/OGDT</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/denotative-description/hasFontStyle -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasFontStyle">
        <rdfs:domain rdf:resource="https://w3id.org/arco/denotative-description/Mark"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/denotative-description/FontStyle"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;iscrizione, uno stemma, un timbro, etc., al tipo di caratteri che viene adottato (es.: corsivo, grassetto, caratteri gotici, etc.).</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/denotative-description/"/>
        <rdfs:label xml:lang="it">ha tipo di caratteri</rdfs:label>
        <rdfs:label xml:lang="en">has font style</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag xml:lang="it">ICCD Normativa Traversale 4.00: DA/ISE/ISET - Normative di versioni precedenti: DA/ISR/ISRT</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/denotative-description/hasLocationSubject -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasLocationSubject">
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="http://www.w3id.org/arco/location/TimeIndexedQualifiedLocation"/>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta l&apos;area rappresentata come soggetto di un bene culturale.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/denotative-description/"/>
        <rdfs:label xml:lang="it">ha localizzazione rappresentata</rdfs:label>
        <rdfs:label xml:lang="en">has location as subject</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag xml:lang="it">se 4.00 LA/TLC o versioni precedenti a 4.00 LA/TCL = &quot;area rappresentata&quot;</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/denotative-description/hasMark -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasMark">
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/denotative-description/Mark"/>
        <rdfs:label xml:lang="en">has mark</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag xml:lang="it">ICCD Normativa Trasversale 4.00: DA/ISE - ICCD versioni precedenti alla 4.00: DA/ISR; DA/STM - ICCD Normativa A: LI/LSI</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/denotative-description/hasMarkType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasMarkType">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/core/hasType"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/denotative-description/Mark"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;iscrizione, un emblema, un timbro, uno stemma, etc., al suo tipo (es.: gentilizio, civile, religioso, etc.)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/denotative-description/"/>
        <rdfs:label xml:lang="en">has mark type</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag xml:lang="it">ICCD Normativa Trasversale 4.00: DA/ISE/ISEE - Normative di versioni precedenti: DA/STM/STMQ</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/denotative-description/hasMeasurement -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasMeasurement">
        <rdfs:domain rdf:resource="https://w3id.org/arco/denotative-description/MeasurementCollection"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/denotative-description/Measurement"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un insieme di misure osservate per un oggetto a una delle misure.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/denotative-description/"/>
        <rdfs:label xml:lang="it">ha misura</rdfs:label>
        <rdfs:label xml:lang="en">has measurement</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco-core:iccdNormTag xml:lang="it">MT/MIS - Normativa VeAC: anche MT/MIM</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/denotative-description/hasMeasurementCollection -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasMeasurementCollection">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/denotative-description/MeasurementCollection"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un oggetto all&apos;insieme delle misure osservate per quell&apos;oggetto.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/denotative-description/"/>
        <rdfs:label xml:lang="it">ha collezione di misure</rdfs:label>
        <rdfs:label xml:lang="en">has measurement collection</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco-core:iccdNormTag xml:lang="it">MT/MIS - Normativa VeAC: anche MT/MII e MT/MIM</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/denotative-description/hasMeasurementType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasMeasurementType">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/core/hasType"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/denotative-description/Measurement"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/denotative-description/MeasurementType"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una misura al tipo di misura (altezza, larghezza, diametro, etc.)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/denotative-description/"/>
        <rdfs:label xml:lang="it">ha tipo di misura</rdfs:label>
        <rdfs:label xml:lang="en">has measurement type</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <arco-core:iccdNormTag xml:lang="it">ICCD Normativa Trasversale: MT/MIS/MISZ; MT/MNR - Per versioni di normativa precedenti alla 4.00: MT/MIS/MISA; MT/MIS/MISD; MT/MIS/MISL; MT/MIS/MISN; MT/MIS/MISG; MT/MIS/MISP; MT/MIS/MISS; MT/MIS/MISR; MT/MIS/MISV - ICCD scheda VeAC: MT/MII/MIIA; MT/MII/MIIL; MT/MII/MIIP; MT/MIS/MISR; MT/MIS/MISD; MT/MIS/MISS; MT/MIS/MISP; MT/MIS/MISV; MT/MIS/MISF; MT/MIS/MISO</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/denotative-description/hasOrientation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasOrientation">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/denotative-description/Orientation"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un oggetto al suo orientamento, secondo l&apos;asse principale individuato.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/denotative-description/"/>
        <rdfs:label xml:lang="it">ha orientamento</rdfs:label>
        <rdfs:label xml:lang="en">has orientation</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag xml:lang="it">ICCD Normativa Trasversale: MT/ORT - ICCD Schede CA e MA: MT/ORT</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/denotative-description/hasSupport -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasSupport">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/denotative-description/isSupportOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/denotative-description/Mark"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/denotative-description/Support"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una iscrizione, un timbro, un emblema, etc., rappresentato su un bene culturale al supporto su cui si trova l&apos;elemento.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/denotative-description/"/>
        <rdfs:label xml:lang="it">ha supporto</rdfs:label>
        <rdfs:label xml:lang="en">has support</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <arco-core:iccdNormTag>ICCD Normativa Trasversale 4.00: DA/ISE/ISES</arco-core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/denotative-description/hasTechnicalDetailOccurrence -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/denotative-description/hasTechnicalDetailOccurrence">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/denotative-description/TechnicalDetailOccurrence"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità (per esempio, un bene culturale) a un&apos;occorrenza di un dettaglio tecnico, cioè a una situazione in cui un dettaglio tecnico occorre nella realtà in un&apos;entità specifica. Per esempio, una occorrenza di un dettaglio tecnico è: &quot;bene culturale X realizzato con la materia terracotta&quot;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/denotative-description/"/>
        <rdfs:label xml:lang="it">ha occorrenza di dettaglio tecnico</rdfs:label>
        <rdfs:label xml:lang="en">has technical detail occurrence</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
