    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/arco/core/iccdNormTag -->

    <owl:AnnotationProperty rdf:about="https://w3id.org/arco/core/iccdNormTag"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/arco/culturaldefinition/hasAuthor -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasAuthor">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/arco/culturaldefinition/AlternativeAuthorshipAttribution"/>
                    <rdf:Description rdf:about="https://w3id.org/arco/culturaldefinition/PreferredAuthorshipAttribution"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Agent"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una situazione in cui viene attribuito un autore a un bene culturale all&apos;autore stesso.</rdfs:comment>
        <rdfs:label xml:lang="it">autore</rdfs:label>
        <rdfs:label xml:lang="en">has author</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>AU/AUT/AUTN (+ AU/AUT/AUTS); AU/AAT; AU/AAT/AATN</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasAuthorityFileCataloguingAgency -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasAuthorityFileCataloguingAgency">
        <rdfs:domain rdf:resource="https://w3id.org/arco/culturaldefinition/PreferredAuthorshipAttribution"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Agent"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega l&apos;attribuzione di un autore a un bene culturale all&apos;ente schedatore dell&apos;authority file relativo a quell&apos;autore. Questa proprietà, esprimendo il significato della relazione nel nome, dà una rappresentazione semplificata della relazione che lega l&apos;attribuzione, l&apos;authority file, e l&apos;ente schedatore dell&apos;authority file.</rdfs:comment>
        <rdfs:label xml:lang="it">ha ente schedatore dell&apos;authority file</rdfs:label>
        <rdfs:label xml:lang="en">has authority file cataloguing agency</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>AU/AUT/AUTJ</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasAuthorshipAttribution -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasAuthorshipAttribution">
        <rdfs:range rdf:resource="https://w3id.org/arco/culturaldefinition/AuthorshipAttribution"/>
        <rdfs:comment xml:lang="it">Proprietà che lega un oggetto culturale a un&apos;attribuzione di autore o di responsabilità relativa all&apos;oggetto.</rdfs:comment>
        <rdfs:label xml:lang="it">ha attribuzione di autore/responsabilità</rdfs:label>
        <rdfs:label xml:lang="en">has authorship attribution</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>AU/AUT; AU/ATB; AU/AAT</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasCircumstance -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasCircumstance">
        <rdfs:domain rdf:resource="https://w3id.org/arco/culturaldefinition/Commission"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/culturaldefinition/Circumstance"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una situazione in cui viene indicata la committenza di un bene culturale all&apos;occasione che ha determinato quella committenza</rdfs:comment>
        <rdfs:label xml:lang="it">ha circostanza di committenza</rdfs:label>
        <rdfs:label xml:lang="en">has commission circumstance</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>AU/CMM/CMMC</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasCollection -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasCollection">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/culturaldefinition/isCollectionIn"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/culturaldefinition/CollectionMembership"/>
        <rdfs:range rdf:resource="http://dati.beniculturali.it/cis/CollectionOfCulturalEntities"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega l&apos;appartenenza di un bene culturale a una collezione in un intervallo di tempo alla collezione coinvolta nella relazione</rdfs:comment>
        <rdfs:label xml:lang="it">ha collezione</rdfs:label>
        <rdfs:label xml:lang="en">has collection</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>UB/COL/COLD</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasCollector -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasCollector">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/culturaldefinition/isCollectorOf"/>
        <rdfs:domain rdf:resource="http://dati.beniculturali.it/cis/CollectionOfCulturalEntities"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Agent"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un collezione di beni culturali al collezionista.</rdfs:comment>
        <rdfs:label xml:lang="it">ha collezionista</rdfs:label>
        <rdfs:label xml:lang="en">has collector</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>UB/COL/COLN</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasCommission -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasCommission">
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/culturaldefinition/Commission"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un bene culturale alla sua committenza.</rdfs:comment>
        <rdfs:label xml:lang="it">ha committenza</rdfs:label>
        <rdfs:label xml:lang="en">has commission</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>AU/CMM</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasCommittent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasCommittent">
        <rdfs:domain rdf:resource="https://w3id.org/arco/culturaldefinition/Commission"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Agent"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una situazione in cui viene indicata la committenza di un bene culturale alla persona o istituzione committente.</rdfs:comment>
        <rdfs:label xml:lang="it">ha committente</rdfs:label>
        <rdfs:label xml:lang="en">has committent</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>AU/CMM/CMMN</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasCulturalProperty -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasCulturalProperty">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/culturaldefinition/isMemberOfCollection"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/culturaldefinition/CollectionMembership"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/core/MovableCulturalProperty"/>
        <rdfs:comment xml:lang="it">Questa proprietà, inversa a &quot;ha membro di collezione&quot;, collega l&apos;appartenenza di un bene culturale a una collezione in un certo intervallo di tempo al bene culturale mobile coinvolto nella relazione.</rdfs:comment>
        <rdfs:label xml:lang="en">has cultural property</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>UB/COL</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasCulturalScope -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasCulturalScope">
        <rdfs:domain rdf:resource="https://w3id.org/arco/culturaldefinition/CulturalScopeAttribution"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/culturaldefinition/CulturalScope"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una situazione in cui viene attribuito un ambito culturale a un bene culturale all&apos;ambito culturale stesso.</rdfs:comment>
        <rdfs:label xml:lang="it">ambito culturale</rdfs:label>
        <rdfs:label xml:lang="en">has cultural scope</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>AU/ATB</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasDating -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasDating">
        <rdfs:range rdf:resource="https://w3id.org/arco/culturaldefinition/Dating"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità culturale alla sua cronologia.</rdfs:comment>
        <rdfs:label xml:lang="it">ha cronologia</rdfs:label>
        <rdfs:label xml:lang="en">has dating</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>DT</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasEdition -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasEdition">
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/HistoricOrArtisticProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/culturaldefinition/Edition"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un bene storico e artistico a una sua edizione.</rdfs:comment>
        <rdfs:label xml:lang="it">ha edizione</rdfs:label>
        <rdfs:label xml:lang="en">has edition</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasEvent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasEvent">
        <rdfs:domain rdf:resource="https://w3id.org/arco/culturaldefinition/Dating"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Event"/>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta l&apos;evento relativo al bene culturale, cui si riferisce una specifica cronologia.</rdfs:comment>
        <rdfs:label xml:lang="en">has event related</rdfs:label>
        <rdfs:label xml:lang="it">relativa a evento</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>DTN/DTNS; se non c&apos;è, Event = &quot;realizzazione&quot;</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasInterventionRole -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasInterventionRole">
        <rdfs:domain rdf:resource="https://w3id.org/arco/culturaldefinition/AuthorshipAttribution"/>
        <rdfs:range rdf:resource="http://dati.beniculturali.it/roapit/Role"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa un&apos;attribuzione di autore/responsabilità al ruolo rivestito dall&apos;autore o dall&apos;ambito culturale.</rdfs:comment>
        <rdfs:label xml:lang="en">intervention role</rdfs:label>
        <rdfs:label xml:lang="it">ruolo nell&apos;intervento</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>AU/ATB/ATBR; AU/AUT/AUTR</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasInventory -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasInventory">
        <rdfs:range rdf:resource="https://w3id.org/arco/culturaldefinition/Inventory"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un bene culturale a un inventario che registra informazioni relative al bene culturale.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a cultural property to an inventory about it.</rdfs:comment>
        <rdfs:label xml:lang="it">ha inventario</rdfs:label>
        <rdfs:label xml:lang="en">has inventory</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag xml:lang="it">UB/INV
ICCD Normativa Trasversale: UB/INP</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasResponsibleInventoryIdentifier -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasResponsibleInventoryIdentifier">
        <rdfs:domain rdf:resource="https://w3id.org/arco/culturaldefinition/Inventory"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Agent"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un inventario al contenitore giuridico che conserva attualmente il bene o che lo ha conservato in passato, e che è responsabile dell&apos;assegnazione del numero di inventario.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates an inventory to the juridical container responsible for the inventory identifier of the cultural property.</rdfs:comment>
        <rdfs:label xml:lang="it">ha responsabile di numero di inventario</rdfs:label>
        <rdfs:label xml:lang="en">has responsible for inventory identifier</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag xml:lang="it">ICCD Normativa Trasversale: UB/INV/INVG</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasSource -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasSource">
        <rdfs:range rdf:resource="https://w3id.org/arco/culturaldefinition/Source"/>
        <rdfs:label xml:lang="it">Questa proprietà associa una attribuzione di autore/responsabilità, una cronologia, una committenza, alla motivazione o fonte che hanno determinato l&apos;attribuzione o l&apos;individuazione di una informazione.</rdfs:label>
        <rdfs:label xml:lang="it">ha motivazione</rdfs:label>
        <rdfs:label xml:lang="en">has source</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>AU/AUT/AUTM; AU/CMM/CMMF; AU/AAT/AATM; AU/ATB/ATBM; DT/DTM/DTMM; SG/SGT/SGTL</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasSubject -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasSubject">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/culturaldefinition/isSubjectOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/culturaldefinition/Subject"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un bene culturale al suo soggetto o motivo ornamentale.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a cultural property to ist subject or ornamental motif.</rdfs:comment>
        <rdfs:label xml:lang="it">ha soggetto</rdfs:label>
        <rdfs:label xml:lang="en">has subject</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag xml:lang="it">ICCD Normativa Trasversale: DA/AID/AIDI - Normativa F: SG/SGT/SGTI - Altre normative di versioni precedenti alla 4.00: OG/SGT/SGTI</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasTitle -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasTitle">
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/culturaldefinition/Title"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un bene culturale al titolo (proprio, attribuito, parallelo, alternativo) del soggetto.</rdfs:comment>
        <rdfs:label xml:lang="it">ha titolo</rdfs:label>
        <rdfs:label xml:lang="en">has title</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>OG/SGT/SGTP; SG/SGL/SGLT; SG/SGL/SGLL; OG/SGT/SGTR; DA/AID/AIDN; SG/SGL/SGLA; DA/AID/AIDT; OG/SGT/SGTT</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/hasTitleType -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/hasTitleType">
        <rdfs:domain rdf:resource="https://w3id.org/arco/culturaldefinition/Title"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/culturaldefinition/TitleType"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un titolo di un bene culturale al tipo di titolo.</rdfs:comment>
        <rdfs:label xml:lang="it">ha tipo di titolo</rdfs:label>
        <rdfs:label xml:lang="en">has title type</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>OG/SGT/SGTP; SG/SGL/SGLT; SG/SGL/SGLL; OG/SGT/SGTR; DA/AID/AIDN; SG/SGL/SGLA; DA/AID/AIDT; OG/SGT/SGTT</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/isCollectionIn -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/isCollectionIn">
        <rdfs:domain rdf:resource="http://dati.beniculturali.it/cis/CollectionOfCulturalEntities"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/culturaldefinition/CollectionMembership"/>
        <rdfs:comment xml:lang="it">Questa proprietà, inversa a &quot;ha collezione&quot;, rappresenta la partecipazione di una collezione nel rapporto tra una collezione e un bene culturale che ne fa parte, in un certo intervallo di tempo.</rdfs:comment>
        <rdfs:label xml:lang="en">is collection in</rdfs:label>
        <rdfs:label xml:lang="it">è collezione in</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>UB/COL/COLD</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/isCollectorOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/isCollectorOf">
        <rdfs:domain rdf:resource="https://w3id.org/italia/onto/l0/Agent"/>
        <rdfs:range rdf:resource="http://dati.beniculturali.it/cis/CollectionOfCulturalEntities"/>
        <rdfs:comment xml:lang="it">Questa proprietà, inversa a &quot;ha collezionista&quot;, collega un collezionista alla relativa collezione di beni culturali.</rdfs:comment>
        <rdfs:label xml:lang="en">is collector of</rdfs:label>
        <rdfs:label xml:lang="it">è collezionista di</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>UB/COL/COLN</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/isMemberOfCollection -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/isMemberOfCollection">
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/MovableCulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/culturaldefinition/CollectionMembership"/>
