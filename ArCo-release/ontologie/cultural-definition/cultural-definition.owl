        <rdfs:label xml:lang="en">is member of collection</rdfs:label>
        <rdfs:label xml:lang="it">è membro di collezione</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>UB/COL</core:iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/culturaldefinition/isSubjectOf -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/culturaldefinition/isSubjectOf">
