        <rdfs:label xml:lang="en">author local identifier</rdfs:label>
        <rdfs:label xml:lang="it">codice identificativo locale dell&apos;autore</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:DatatypeProperty>
    


