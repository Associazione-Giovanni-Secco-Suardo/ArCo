        <rdfs:label xml:lang="it">Attribuzione Alternativa di Autore</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>AU/AAT</core:iccdNormTag>
    </owl:Class>
    


    <!-- https://w3id.org/arco/culturaldefinition/AuthorshipAttribution -->

    <owl:Class rdf:about="https://w3id.org/arco/culturaldefinition/AuthorshipAttribution">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/arco/culturaldefinition/hasAuthorityFileCataloguingAgency"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/italia/onto/l0/Agent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/arco/culturaldefinition/hasInterventionRole"/>
                <owl:allValuesFrom rdf:resource="http://dati.beniculturali.it/roapit/Role"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/arco/culturaldefinition/hasSource"/>
                <owl:allValuesFrom rdf:resource="https://w3id.org/arco/culturaldefinition/Source"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="it">Questa classe rappresenta l&apos;attribuzione di un autore o di una responsabilità per un bene culturale, cioè le informazioni relative ai soggetti (persone singole o enti collettivi) intervenuti nella realizzazione del bene e/o all&apos;ambito culturale di riferimento.</rdfs:comment>
        <rdfs:label xml:lang="it">Attribuzione di Autore o responsabilità</rdfs:label>
        <rdfs:label xml:lang="en">Authorship Attribution</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>ICCD Normativa Trasversale: AU/AAT; AU/ATB; AU/AUT</core:iccdNormTag>
    </owl:Class>
    


    <!-- https://w3id.org/arco/culturaldefinition/Circumstance -->

    <owl:Class rdf:about="https://w3id.org/arco/culturaldefinition/Circumstance">
        <rdfs:comment xml:lang="it">Questa classe rappresenta l&apos;occasione per cui è stato commissionato un bene culturale.</rdfs:comment>
        <rdfs:label xml:lang="it">Circostanza</rdfs:label>
        <rdfs:label xml:lang="en">Circumstance</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <core:iccdNormTag>AU/CMM/CMMC</core:iccdNormTag>
    </owl:Class>
    


    <!-- https://w3id.org/arco/culturaldefinition/CollectionMembership -->

    <owl:Class rdf:about="https://w3id.org/arco/culturaldefinition/CollectionMembership">
