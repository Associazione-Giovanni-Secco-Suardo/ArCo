    <!ENTITY CLV "https://w3id.org/italia/onto/CLV/" >
    <!ENTITY subjective "https://w3id.org/arco/subjective/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY ADMS "https://w3id.org/italia/onto/ADMS/" >
