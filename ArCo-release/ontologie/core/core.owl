    <!-- https://w3id.org/arco/core/CulturalPropertyPart -->

    <owl:Class rdf:about="https://w3id.org/arco/core/CulturalPropertyPart">
        <rdfs:subClassOf rdf:resource="http://dati.beniculturali.it/cis/CulturalEntity"/>
        <rdfs:comment xml:lang="it">Questa classe rappresenta, a un livello generico, una parte di un bene culturale descritto in una scheda cui si possono riferire informazioni specifiche.</rdfs:comment>
        <rdfs:comment xml:lang="en">This class represents a generic part of a cultural property.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/core/"/>
        <rdfs:label xml:lang="en">Cultural Property Part</rdfs:label>
        <rdfs:label xml:lang="it">Parte di Bene Culturale</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <iccdNormTag xml:lang="it">&quot;Riferimento alla parte&quot;, presente in molti campi strutturati delle normative ICCD</iccdNormTag>
    </owl:Class>
    


    <!-- https://w3id.org/arco/core/CulturalPropertyResidual -->

    <owl:Class rdf:about="https://w3id.org/arco/core/CulturalPropertyResidual">
