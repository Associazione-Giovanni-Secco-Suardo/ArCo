    <!-- https://w3id.org/arco/core/hasAgent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/core/hasAgent">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/core/isAgentOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/AgentRole"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Agent"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega una situazione in cui un ruolo viene ricoperto da un agente all&apos;agente che lo ricopre.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a situation in which an agent holds a role to that agent.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/core/"/>
        <rdfs:label xml:lang="it">ha agente</rdfs:label>
        <rdfs:label xml:lang="en">has agent</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasAgentRole -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/core/hasAgentRole">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/core/AgentRole"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità a una situazione in cui un agente ricopre un ruolo.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates an entity to a situation in which an agent holds a role connected to that entity.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/core/"/>
        <rdfs:label xml:lang="it">ha ruolo ricoperto da agente</rdfs:label>
        <rdfs:label xml:lang="en">has agent role</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasAlternativeDiscipline -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/core/hasAlternativeDiscipline">
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="http://dati.beniculturali.it/cis/SubjectDiscipline"/>
        <rdfs:comment xml:lang="it">Questa proprietà permette di associare un bene culturale a discipline scientifiche, alternative a quella principale, cui fa riferimento.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/core/"/>
        <rdfs:label xml:lang="it">ha altra disciplina</rdfs:label>
        <rdfs:label xml:lang="en">has alternative discipline</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <iccdNormTag>ICCD Normativa PST: CT/CTA</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasCataloguingAgency -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/core/hasCataloguingAgency">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/core/hasRelatedAgency"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Agent"/>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta la relazione che collega un bene culturale al relativo ente schedatore. La relazione tra il ruolo e l&apos;ente che lo riveste è espressa nel nome della proprietà, che quindi ne fornisce una rappresentazione semplificata, con ridotta espressività. Nell&apos;ontologia questa relazione è modellata anche, in maniera più espressiva, dalla classe AgentRole, che permette di collegare l&apos;ente e il suo ruolo.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/core/"/>
        <rdfs:label xml:lang="it">ha ente schedatore</rdfs:label>
        <rdfs:label xml:lang="en">has cataloguing agency</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <iccdNormTag>ICCD Normativa Trasversale 4.00: CD/ESC</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasCategory -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/core/hasCategory">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/core/Category"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa un&apos;entità di qualsiasi tipo alla sua categoria. Viene utilizzata, per esempio, per collegare un bene culturale alla sua categoria, oppure una particolare documentazione alla categoria in cui è classificata.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/core/"/>
        <rdfs:label xml:lang="it">ha categoria</rdfs:label>
        <rdfs:label xml:lang="en">has category</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasCulturalPropertyComponent -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/core/hasCulturalPropertyComponent">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/core/hasPart"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/core/isCulturalPropertyComponentOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/core/CulturalPropertyComponent"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/core/"/>
        <rdfs:label xml:lang="it">ha parte componente di bene culturale</rdfs:label>
        <rdfs:label xml:lang="en">has cultural property component</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <iccdNormTag>OG/OGT/OGTP</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasCulturalPropertyResidual -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/core/hasCulturalPropertyResidual">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/core/hasPart"/>
        <owl:inverseOf rdf:resource="https://w3id.org/arco/core/isCulturalPropertyResidualOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://w3id.org/arco/core/CulturalPropertyResidual"/>
                    <rdf:Description rdf:about="https://w3id.org/arco/core/SomeCulturalPropertyResiduals"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/core/"/>
        <rdfs:label xml:lang="it">ha parte residuale di bene culturale</rdfs:label>
        <rdfs:label xml:lang="en">has cultural property residual</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <iccdNormTag>OG/OGT/OGTW</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasHeritageProtectionAgency -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/core/hasHeritageProtectionAgency">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/arco/core/hasRelatedAgency"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Agent"/>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta la relazione che collega un bene culturale al relativo ente competente per la tutela. La relazione tra il ruolo e l&apos;ente che lo riveste è espressa nel nome della proprietà, che quindi ne fornisce una rappresentazione semplificata, con ridotta espressività. Nell&apos;ontologia questa relazione è modellata anche, in maniera più espressiva, dalla classe AgentRole, che permette di collegare l&apos;ente e il suo ruolo.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/core/"/>
        <rdfs:label xml:lang="it">ha ente competente per la tutela</rdfs:label>
        <rdfs:label xml:lang="en">has heritage protection agency</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <iccdNormTag>ICCD Normativa Trasversale 4.00: CD/ECP</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasKeyword -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/core/hasKeyword">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/core/Keyword"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità a una parola chiave relativa, che può essere utilizzata per la ricerca dell&apos;entità.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/core/"/>
        <rdfs:label xml:lang="it">ha parola chiave</rdfs:label>
        <rdfs:label xml:lang="en">has keyword</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <iccdNormTag>ICCD Normativa PST: CT/CTC</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasLocation -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/core/hasLocation">
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/CLV/Feature"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa un&apos;entità a un&apos;area geografica in cui l&apos;entità è collocata.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/core/"/>
        <rdfs:label xml:lang="it">ha luogo</rdfs:label>
        <rdfs:label xml:lang="en">has location</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <iccdNormTag xml:lang="it">DO/MST/MSTL (versioni precedenti a 4.00)</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasMainDiscipline -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/core/hasMainDiscipline">
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="http://dati.beniculturali.it/cis/SubjectDiscipline"/>
        <rdfs:comment xml:lang="it">Questa proprietà permette di associare un bene culturale alla disciplina scientifica più importante, o storicamente riconosciuta, cui fa riferimento.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/core/"/>
        <rdfs:label xml:lang="it">ha disciplina principale</rdfs:label>
        <rdfs:label xml:lang="en">has main discipline</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <iccdNormTag>ICCD Normativa PST: CT/CTP</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasMibactScopeOfProtection -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/core/hasMibactScopeOfProtection">
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/core/MibactScopeOfProtection"/>
        <rdfs:comment xml:lang="it">Questa proprietà associa un bene culturale all&apos;ambito di tutela nel quale ricade.</rdfs:comment>
        <rdfs:comment xml:lang="en">This property relates a cultural property to ist MiBACT scope of protection</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/core/"/>
        <rdfs:label xml:lang="it">ha ambito di tutela</rdfs:label>
        <rdfs:label xml:lang="en">has MiBACT scope of protection</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasNumberOfComponents -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/core/hasNumberOfComponents">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/core/isNumberOfComponentsOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/arco/core/NumberOfComponents"/>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/core/"/>
        <rdfs:label xml:lang="it">ha numero di componenti</rdfs:label>
        <rdfs:label xml:lang="en">has number of components</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <iccdNormTag>OG/QNT/QNTN; OG/QNT/QNTI; OG/QNT/QNTS</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasPart -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/core/hasPart">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/core/isPartOf"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <opla:isNativeTo rdf:resource="http://www.ontologydesignpatterns.org/cp/owl/partof.owl"/>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità a una sua parte. Per esempio, può collegare un bene culturale a una sua parte (es.: piano terra, manico, etc.)</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://w3id.org/arco/core/"/>
        <rdfs:label xml:lang="it">ha parte</rdfs:label>
        <rdfs:label xml:lang="en">has part</rdfs:label>
        <owl:versionInfo xml:lang="it">stabile</owl:versionInfo>
        <owl:versionInfo xml:lang="en">stable</owl:versionInfo>
        <iccdNormTag xml:lang="it">&quot;Riferimento alla parte&quot;, presente in molti campi strutturati delle normative ICCD</iccdNormTag>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasRelatedAgency -->

    <owl:ObjectProperty rdf:about="https://w3id.org/arco/core/hasRelatedAgency">
        <owl:inverseOf rdf:resource="https://w3id.org/arco/core/isRelatedAgencyOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/arco/core/CulturalProperty"/>
        <rdfs:range rdf:resource="https://w3id.org/italia/onto/l0/Agent"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/arco/core/hasAgentRole"/>
            <rdf:Description rdf:about="https://w3id.org/arco/core/hasAgent"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta la relazione che collega un bene culturale a un ente che riveste un ruolo nei suoi confronti. La relazione tra il ruolo e l&apos;ente che lo riveste è espressa, in maniera generica, nel nome della proprietà, che quindi ne fornisce una rappresentazione semplificata, con ridotta espressività. La proprietà è specializzata per mezzo di sottoproprietà. Nell&apos;ontologia questa relazione è modellata anche, in maniera più espressiva, dalla classe TimeIndexedRole, che permette di collegare l&apos;ente, il suo ruolo e la sua corrispondente estensione temporale.</rdfs:comment>
