        <rdfs:label xml:lang="it">Categoria del bene</rdfs:label>
        <rdfs:label xml:lang="en">Cultural Property Category</rdfs:label>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
    </owl:Class>
    


    <!-- https://w3id.org/arco/core/CulturalPropertyComponent -->

    <owl:Class rdf:about="https://w3id.org/arco/core/CulturalPropertyComponent">
