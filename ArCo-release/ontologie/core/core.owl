<rdf:RDF xmlns="https://w3id.org/arco/core/"
     xml:base="https://w3id.org/arco/core/"
     xmlns:locgeoamm="https://w3id.org/arco/location/"
     xmlns:subjective="https://w3id.org/arco/subjective/"
     xmlns:l0="https://w3id.org/italia/onto/l0/"
     xmlns:roapit="https://w3id.org/italia/onto/RO/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:ns="http://www.w3.org/2006/vcard/ns#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:cis="http://dati.beniculturali.it/cis/"
     xmlns:CLV="https://w3id.org/italia/onto/CLV/"
     xmlns:objective="https://w3id.org/arco/objective/"
     xmlns:arco="https://w3id.org/arco/core/"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:terms="http://purl.org/dc/terms/"
     xmlns:ADMS="https://w3id.org/italia/onto/ADMS/"
     xmlns:dcat="http://www.w3.org/ns/dcat#"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="https://w3id.org/arco/core/">
        <dc:creator>Aldo Gangemi (ISTC-CNR, STLab)</dc:creator>
        <dc:creator>Marialetizia Mancinelli (MiBACT, ICCD)</dc:creator>
        <dc:creator>Valentina Carriero (ISTC-CNR, STLab)</dc:creator>
        <dc:creator>Valentina Presutti (ISTC-CNR, STLab)</dc:creator>
        <dc:creator>Andrea Nuzzolese (ISTC-CNR, STLab)</dc:creator>
        <dc:creator>Margherita Porena (MiBACT, ICCD)</dc:creator>
        <dc:creator>Chiara Veninata (MiBACT, ICCD)</dc:creator>
        <rdfs:comment xml:lang="it">Questo è il modulo core della rete di ontologie di ArCo - Architettura della Conoscenza. Qui si modellano informazioni, relative al bene culturale, considerate centrali. Vengono importati tutti gli altri moduli della rete.</rdfs:comment>
        <owl:versionInfo xml:lang="it">Versione 0.2 beta</owl:versionInfo>
        <owl:imports rdf:resource="http://dati.beniculturali.it/cis/"/>
        <owl:imports rdf:resource="https://w3id.org/arco/subjective/"/>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/arco/core/iccdNormTag -->

    <owl:AnnotationProperty rdf:about="&arco;iccdNormTag">
        <rdfs:label xml:lang="en">ICCD norm tag</rdfs:label>
        <rdfs:label xml:lang="it">acronimo normativa ICCD</rdfs:label>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà permette di annotare il campo o i campi della normativa ICCD corrispondenti a una determinata classe o proprietà dell&apos;ontologia.</rdfs:comment>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
    </owl:AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://dati.beniculturali.it/cis/isRelatedToRiT -->

    <owl:ObjectProperty rdf:about="&cis;isRelatedToRiT"/>
    


    <!-- https://w3id.org/arco/core/hasAgent -->

    <owl:ObjectProperty rdf:about="&arco;hasAgent">
        <rdfs:label xml:lang="en">has agent</rdfs:label>
        <rdfs:label xml:lang="it">ha agente</rdfs:label>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà collega una situazione in cui un ruolo viene ricoperto da un agente all&apos;agente che lo ricopre.</rdfs:comment>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:domain rdf:resource="&arco;AgentRole"/>
        <owl:inverseOf rdf:resource="&arco;isAgentOf"/>
        <rdfs:range rdf:resource="&l0;Agent"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasAgentRole -->

    <owl:ObjectProperty rdf:about="&arco;hasAgentRole">
        <rdfs:label xml:lang="en">has agent role</rdfs:label>
        <rdfs:label xml:lang="it">ha ruolo ricoperto da agente</rdfs:label>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità a una situazione in cui un agente ricopre un ruolo.</rdfs:comment>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:domain rdf:resource="&owl;Thing"/>
        <rdfs:range rdf:resource="&arco;AgentRole"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasCataloguingAgency -->

    <owl:ObjectProperty rdf:about="&arco;hasCataloguingAgency">
        <rdfs:label xml:lang="en">has cataloguing agency</rdfs:label>
        <rdfs:label xml:lang="it">ha ente schedatore</rdfs:label>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta la relazione che collega un bene culturale al relativo ente schedatore. La relazione tra il ruolo e l&apos;ente che lo riveste è espressa nel nome della proprietà, che quindi ne fornisce una rappresentazione semplificata, con ridotta espressività. Nell&apos;ontologia questa relazione è modellata anche, in maniera più espressiva, dalla classe AgentRole, che permette di collegare l&apos;ente e il suo ruolo.</rdfs:comment>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:domain rdf:resource="&arco;CulturalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&arco;hasRelatedAgency"/>
        <rdfs:range rdf:resource="&l0;Agent"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasCategory -->

    <owl:ObjectProperty rdf:about="&arco;hasCategory">
        <rdfs:label xml:lang="en">has category</rdfs:label>
        <rdfs:label xml:lang="it">ha categoria</rdfs:label>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà associa un&apos;entità di qualsiasi tipo alla sua categoria. Viene utilizzata, per esempio, per collegare un bene culturale alla sua categoria, oppure una particolare documentazione alla categoria in cui è classificata.</rdfs:comment>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:domain rdf:resource="&owl;Thing"/>
        <rdfs:range rdf:resource="&arco;Category"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasCulturalPropertyComponent -->

    <owl:ObjectProperty rdf:about="&arco;hasCulturalPropertyComponent">
        <rdfs:label xml:lang="en">has cultural property component</rdfs:label>
        <rdfs:label xml:lang="it">ha parte componente di bene culturale</rdfs:label>
        <iccdNormTag>OG/OGT/OGTP</iccdNormTag>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:domain rdf:resource="&arco;CulturalProperty"/>
        <rdfs:range rdf:resource="&arco;CulturalPropertyComponent"/>
        <rdfs:subPropertyOf rdf:resource="&arco;hasPart"/>
        <owl:inverseOf rdf:resource="&arco;isCulturalPropertyComponentOf"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasCulturalPropertyResidual -->

    <owl:ObjectProperty rdf:about="&arco;hasCulturalPropertyResidual">
        <rdfs:label xml:lang="en">has cultural property residual</rdfs:label>
        <rdfs:label xml:lang="it">ha parte residuale di bene culturale</rdfs:label>
        <iccdNormTag>OG/OGT/OGTW</iccdNormTag>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:domain rdf:resource="&arco;CulturalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&arco;hasPart"/>
        <owl:inverseOf rdf:resource="&arco;isCulturalPropertyResidualOf"/>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&arco;CulturalPropertyResidual"/>
                    <rdf:Description rdf:about="&arco;SomeCulturalPropertyResiduals"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasHeritageProtectionAgency -->

    <owl:ObjectProperty rdf:about="&arco;hasHeritageProtectionAgency">
        <rdfs:label xml:lang="en">has heritage protection agency</rdfs:label>
        <rdfs:label xml:lang="it">ha ente competente per la tutela</rdfs:label>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta la relazione che collega un bene culturale al relativo ente competente per la tutela. La relazione tra il ruolo e l&apos;ente che lo riveste è espressa nel nome della proprietà, che quindi ne fornisce una rappresentazione semplificata, con ridotta espressività. Nell&apos;ontologia questa relazione è modellata anche, in maniera più espressiva, dalla classe AgentRole, che permette di collegare l&apos;ente e il suo ruolo.</rdfs:comment>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:domain rdf:resource="&arco;CulturalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&arco;hasRelatedAgency"/>
        <rdfs:range rdf:resource="&l0;Agent"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasMibactScopeOfProtection -->

    <owl:ObjectProperty rdf:about="&arco;hasMibactScopeOfProtection">
        <rdfs:label xml:lang="en">has MiBACT scope of protection</rdfs:label>
        <rdfs:label xml:lang="it">ha ambito di tutela</rdfs:label>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà associa un bene culturale all&apos;ambito di tutela nel quale ricade.</rdfs:comment>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:domain rdf:resource="&arco;CulturalProperty"/>
        <rdfs:range rdf:resource="&arco;MibactScopeOfProtection"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasNumberOfComponents -->

    <owl:ObjectProperty rdf:about="&arco;hasNumberOfComponents">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">has number of components</rdfs:label>
        <rdfs:label xml:lang="it">ha numero di componenti</rdfs:label>
        <iccdNormTag>OG/QNT/QNTN; OG/QNT/QNTI; OG/QNT/QNTS</iccdNormTag>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:domain rdf:resource="&arco;CulturalProperty"/>
        <rdfs:range rdf:resource="&arco;NumberOfComponents"/>
        <owl:inverseOf rdf:resource="&arco;isNumberOfComponentsOf"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasPart -->

    <owl:ObjectProperty rdf:about="&arco;hasPart">
        <owl:inverseOf rdf:resource="&arco;isPartOf"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasRelatedAgency -->

    <owl:ObjectProperty rdf:about="&arco;hasRelatedAgency">
        <rdfs:label xml:lang="en">has related agency</rdfs:label>
        <rdfs:label xml:lang="it">ha ente collegato</rdfs:label>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà rappresenta la relazione che collega un bene culturale a un ente che riveste un ruolo nei suoi confronti. La relazione tra il ruolo e l&apos;ente che lo riveste è espressa, in maniera generica, nel nome della proprietà, che quindi ne fornisce una rappresentazione semplificata, con ridotta espressività. La proprietà è specializzata per mezzo di sottoproprietà. Nell&apos;ontologia questa relazione è modellata anche, in maniera più espressiva, dalla classe TimeIndexedRole, che permette di collegare l&apos;ente, il suo ruolo e la sua corrispondente estensione temporale.</rdfs:comment>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:domain rdf:resource="&arco;CulturalProperty"/>
        <rdfs:range rdf:resource="&l0;Agent"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="&arco;hasAgentRole"/>
            <rdf:Description rdf:about="&arco;hasAgent"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasRole -->

    <owl:ObjectProperty rdf:about="&arco;hasRole">
        <rdfs:label xml:lang="en">has role</rdfs:label>
        <rdfs:label xml:lang="it">ha ruolo</rdfs:label>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà collega una situazione in cui un ruolo viene ricoperto da un agente al ruolo stesso.</rdfs:comment>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:range rdf:resource="&arco;AgentRole"/>
        <owl:inverseOf rdf:resource="&arco;isRoleOf"/>
        <rdfs:domain rdf:resource="&roapit;Role"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/hasType -->

    <owl:ObjectProperty rdf:about="&arco;hasType">
        <rdfs:label xml:lang="en">has type</rdfs:label>
        <rdfs:label xml:lang="it">ha tipo</rdfs:label>
        <iccdNormTag>ICCD Normativa Trasversale 4.00: Denominazione OG/OGD/OGDT - ICCD Normativa Trasversale: Misura MT/MIS/MISZ; MT/MNR - Per versioni di normativa precedenti alla 4.00: MT/MIS/MISA; MT/MIS/MISD; MT/MIS/MISL; MT/MIS/MISN; MT/MIS/MISG; MT/MIS/MISP; MT/MIS/MISS; MT/MIS/MISR; MT/MIS/MISV - ICCD scheda VeAC: MT/MII/MIIA; MT/MII/MIIL; MT/MII/MIIP; MT/MIS/MISR; MT/MIS/MISD; MT/MIS/MISS; MT/MIS/MISP; MT/MIS/MISV; MT/MIS/MISF; MT/MIS/MISO - ICCD Normativa Trasversale 4.00: Stato di conservazione CO/STC/STCC - ICCD Normativa Trasversale 4.00: Tipo di bene OG/OGT</iccdNormTag>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà collega un&apos;entità di qualsiasi tipo a una sua tipologia. Per esempio, nell&apos;ambito delle normative dell&apos;ICCD, può collegare: uno stato di conservazione di un bene culturale a uno dei valori del tipo di stato di conservazione (buono, cattivo, mediocre, etc.); un bene culturale alla sua tipologia (OGTD + OGTT); una denominazione o dedicazione, o, per la normativa FF, il titolo nel tempo, al tipo di denominazione del bene (altra lingua, attribuita, attuale, dialettale, etc.); una misura al tipo di misura (altezza, larghezza, diametro, etc.).</rdfs:comment>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:domain rdf:resource="&owl;Thing"/>
        <rdfs:range rdf:resource="&arco;Type"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/isAgentOf -->

    <owl:ObjectProperty rdf:about="&arco;isAgentOf">
        <rdfs:label xml:lang="en">is agent of</rdfs:label>
        <rdfs:label xml:lang="it">è agente in</rdfs:label>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà, inversa a &quot;ha agente&quot;, collega un agente alla situazione in cui l&apos;agente ha ricoperto un ruolo.</rdfs:comment>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:range rdf:resource="&arco;AgentRole"/>
        <rdfs:domain rdf:resource="&l0;Agent"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/isCulturalPropertyComponentOf -->

    <owl:ObjectProperty rdf:about="&arco;isCulturalPropertyComponentOf">
        <rdfs:label xml:lang="en">is cultural property component of</rdfs:label>
        <rdfs:label xml:lang="it">è parte componente di bene culturale</rdfs:label>
        <iccdNormTag>OG/OGT/OGTP</iccdNormTag>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:range rdf:resource="&arco;CulturalProperty"/>
        <rdfs:domain rdf:resource="&arco;CulturalPropertyComponent"/>
        <rdfs:subPropertyOf rdf:resource="&arco;isPartOf"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/isCulturalPropertyResidualOf -->

    <owl:ObjectProperty rdf:about="&arco;isCulturalPropertyResidualOf">
        <rdfs:label xml:lang="en">is cultural property residual of</rdfs:label>
        <rdfs:label xml:lang="it">è parte residuale di bene culturale</rdfs:label>
        <iccdNormTag>OG/OGT/OGTW</iccdNormTag>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:range rdf:resource="&arco;CulturalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&arco;isPartOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="&arco;CulturalPropertyResidual"/>
                    <rdf:Description rdf:about="&arco;SomeCulturalPropertyResiduals"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/isNumberOfComponentsOf -->

    <owl:ObjectProperty rdf:about="&arco;isNumberOfComponentsOf">
        <rdfs:label xml:lang="en">is number of components of</rdfs:label>
        <rdfs:label xml:lang="it">è numero di componenti di</rdfs:label>
        <iccdNormTag>OG/QNT/QNTN; OG/QNT/QNTI; OG/QNT/QNTS</iccdNormTag>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:range rdf:resource="&arco;CulturalProperty"/>
        <rdfs:domain rdf:resource="&arco;NumberOfComponents"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/core/isPartOf -->

    <owl:ObjectProperty rdf:about="&arco;isPartOf"/>
    


    <!-- https://w3id.org/arco/core/isRoleOf -->

    <owl:ObjectProperty rdf:about="&arco;isRoleOf">
        <rdfs:label xml:lang="en">is role of</rdfs:label>
        <rdfs:label xml:lang="it">è ruolo in</rdfs:label>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <owl:versionInfo xml:lang="it">Questa proprietà collega un ruolo a una situazione in cui il ruolo viene ricoperto da un agente.</owl:versionInfo>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/arco/location/hasTimeIndexedQualifiedLocation -->

    <owl:ObjectProperty rdf:about="&locgeoamm;hasTimeIndexedQualifiedLocation"/>
    


    <!-- https://w3id.org/arco/location/hasTypeOfContext -->

    <owl:ObjectProperty rdf:about="&locgeoamm;hasTypeOfContext"/>
    


    <!-- https://w3id.org/arco/objective/hasConservationStatus -->

    <owl:ObjectProperty rdf:about="&objective;hasConservationStatus"/>
    


    <!-- https://w3id.org/arco/objective/hasMeasurements -->

    <owl:ObjectProperty rdf:about="&objective;hasMeasurements"/>
    


    <!-- https://w3id.org/arco/objective/hasOrientation -->

    <owl:ObjectProperty rdf:about="&objective;hasOrientation"/>
    


    <!-- https://w3id.org/arco/subjective/hasAuthorshipAttribution -->

    <owl:ObjectProperty rdf:about="&subjective;hasAuthorshipAttribution"/>
    


    <!-- https://w3id.org/arco/subjective/hasBibliography -->

    <owl:ObjectProperty rdf:about="&subjective;hasBibliography"/>
    


    <!-- https://w3id.org/arco/subjective/hasCommission -->

    <owl:ObjectProperty rdf:about="&subjective;hasCommission"/>
    


    <!-- https://w3id.org/arco/subjective/hasDating -->

    <owl:ObjectProperty rdf:about="&subjective;hasDating"/>
    


    <!-- https://w3id.org/arco/subjective/hasDocumentation -->

    <owl:ObjectProperty rdf:about="&subjective;hasDocumentation"/>
    


    <!-- https://w3id.org/arco/subjective/hasInventory -->

    <owl:ObjectProperty rdf:about="&subjective;hasInventory"/>
    


    <!-- https://w3id.org/arco/subjective/hasTitle -->

    <owl:ObjectProperty rdf:about="&subjective;hasTitle"/>
    


    <!-- https://w3id.org/arco/subjective/isMemberOfCollection -->

    <owl:ObjectProperty rdf:about="&subjective;isMemberOfCollection"/>
    


    <!-- https://w3id.org/italia/onto/RO/isRoleInTimeOf -->

    <owl:ObjectProperty rdf:about="&roapit;isRoleInTimeOf"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://w3id.org/arco/core/catalogueNumber -->

    <owl:DatatypeProperty rdf:about="&arco;catalogueNumber">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en">catalogue number</rdfs:label>
        <rdfs:label xml:lang="it">numero catalogo generale</rdfs:label>
        <owl:versionInfo xml:lang="en">unstable</owl:versionInfo>
        <rdfs:comment xml:lang="it">Questa proprietà permette di associare il numero di catalogo generale a un bene culturale descritto in una scheda catalografica.</rdfs:comment>
        <owl:versionInfo xml:lang="it">provvisoria</owl:versionInfo>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain rdf:resource="&arco;CulturalProperty"/>
        <rdfs:subPropertyOf rdf:resource="&l0;identifier"/>
    </owl:DatatypeProperty>
    


    <!-- https://w3id.org/arco/core/description -->

    <owl:DatatypeProperty rdf:about="&arco;description">
        <rdfs:label xml:lang="en">description</rdfs:label>
